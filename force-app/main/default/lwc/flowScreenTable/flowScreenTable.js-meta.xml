<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>57.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
    <target>lightning__AppPage</target>
    <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="parentRecordId" label="Record Id" type="String" description="Enter Parent Object Record Id" role="inputOnly"/>
            <property name="objectName" label="Object Name" type="String" role="inputOnly" required="true" default="" description="Enter the object Api Name of object"/>
            <property name="fieldReferenceName" label="Field Reference Name" type="String" role="inputOnly"/> 
            <property name="fieldsetName" label="Fieldset Name" type="String" role="inputOnly" required="true" default=""/>
            
            <propertyType name="sObject" extends="SObject" label="Record Object" description="Select record object" />
            <property name="SelectedRecord" type="{sObject[]}" role="outputOnly"/>
        </targetConfig>
